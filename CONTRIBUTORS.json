  "workflow":  {
    "branch_strategy": "feature_branches",
    "naming_convention": "feature/{ai_name}/{description}",
    "pr_requirements": {
      "code_review": "any_ai_collaborator",
      "tests": "required",
      "documentation": "required"
    },
    "merge_strategy": "squash_merge",
    "commit_message_format": "conventional_commits"
  },
  "collaboration_rules": {
    "parallel_development": "allowed",
    "conflict_resolution":  "discussion_required",
    "code_ownership": "shared",
    "security_policy": "strict_review_for_sensitive_code",
    "communication_channel": "pull_request_comments"
  },
  "security":  {
    "require_signed_commits": false,
    "branch_protection": {
      "main": {
        "require_pr_reviews": true,
        "require_status_checks": true,
        "dismiss_stale_pr_approvals": false
      }
    },
    "secret_management": "use_github_secrets",
    "api_token_rotation": "every_90_days"
  },
  "documentation": {
    "contributing_guide": "CONTRIBUTING.md",
    "code_of_conduct": "CODE_OF_CONDUCT.md",
    "development_setup": "docs/SETUP.md",
    "api_documentation": "docs/API. md"
  },
  "notes": {
    "purpose": "Multi-AI collaborative development environment for the Nervous System project",
    "status": "active",
    "last_updated": "2025-12-10"
  }
